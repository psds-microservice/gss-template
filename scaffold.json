{
  "command": {
    "git": [["init"]],
    "go": [
      ["mod", "tidy"],
      ["get", "gorm.io/gorm", "gorm.io/driver/postgres"],
      ["get", "github.com/spf13/cobra@latest"],
      ["get", "github.com/golang-migrate/migrate/v4"],
      ["get", "github.com/joho/godotenv@latest"],
      ["get", "github.com/swaggo/http-swagger@latest"],
      ["get", "github.com/swaggo/files@latest"],
      ["get", "google.golang.org/grpc@latest"],
      ["get", "github.com/grpc-ecosystem/grpc-gateway/v2@latest"],
      ["get", "google.golang.org/genproto/googleapis/api@latest"]
    ]
  },
  "copy": {},
  "gitkeep": [
    "/database/migrations",
    "/database/seeds",
    "/deployments/data",
    "/infra"
  ],
  "mkdir": [
    "/cmd",
    "/cmd/{{.Project.K}}",
    "/internal/application",
    "/internal/command",
    "/internal/config",
    "/internal/consumer",
    "/internal/dto",
    "/internal/errs",
    "/internal/grpc",
    "/internal/handler",
    "/internal/mapper",
    "/internal/model",
    "/internal/database",
    "/internal/service",
    "/internal/validator",
    "/pkg/{{.Project.S}}",
    "/pkg/proto",
    "/pkg/gen",
    "/pkg/gen/proto",
    "/pkg/constants",
    "/third_party",
    "/third_party/google",
    "/third_party/google/api",
    "/database/migrations",
    "/database/seeds",
    "/build",
    "/build/k8s",
    "/build/local",
    "/deployments",
    "/deployments/data",
    "/infra",
    "/api"
  ],
  "render": [
    { "from": "/go.mod.tmpl", "to": "/go.mod" },
    { "from": "/README.md.tmpl", "to": "/README.md" },
    { "from": "/.env.example.tmpl", "to": "/.env.example" },
    { "from": "/.gitignore.tmpl", "to": "/.gitignore" },
    { "from": "/Makefile.tmpl", "to": "/Makefile" },
    { "from": "/cmd/main.go.tmpl", "to": "/cmd/{{.Project.K}}/main.go" },
    { "from": "/cmd/root.go.tmpl", "to": "/cmd/root.go" },
    { "from": "/cmd/api.go.tmpl", "to": "/cmd/api.go" },
    { "from": "/cmd/command.go.tmpl", "to": "/cmd/command.go" },
    { "from": "/cmd/worker.go.tmpl", "to": "/cmd/worker.go" },
    { "from": "/cmd/scheduler.go.tmpl", "to": "/cmd/scheduler.go" },
    { "from": "/cmd/seeder.go.tmpl", "to": "/cmd/seeder.go" },
    { "from": "/internal/application/application.go.tmpl", "to": "/internal/application/application.go" },
    { "from": "/internal/config/config.go.tmpl", "to": "/internal/config/config.go" },
    { "from": "/internal/errs/errs.go.tmpl", "to": "/internal/errs/errs.go" },
    { "from": "/internal/grpc/server.go.tmpl", "to": "/internal/grpc/server.go" },
    { "from": "/internal/model/entity.go.tmpl", "to": "/internal/model/entity.go" },
    { "from": "/pkg/proto/api.proto.tmpl", "to": "/pkg/proto/api.proto" },
    { "from": "/third_party/google/api/annotations.proto.tmpl", "to": "/third_party/google/api/annotations.proto" },
    { "from": "/third_party/google/api/http.proto.tmpl", "to": "/third_party/google/api/http.proto" },
    { "from": "/internal/database/database.go.tmpl", "to": "/internal/database/database.go" },
    { "from": "/internal/database/migrate.go.tmpl", "to": "/internal/database/migrate.go" },
    { "from": "/internal/database/seed.go.tmpl", "to": "/internal/database/seed.go" },
    { "from": "/internal/handler/healthcheck.go.tmpl", "to": "/internal/handler/healthcheck.go" },
    { "from": "/internal/service/service.go.tmpl", "to": "/internal/service/service.go" },
    { "from": "/internal/command/command.go.tmpl", "to": "/internal/command/command.go" },
    { "from": "/internal/consumer/consumer.go.tmpl", "to": "/internal/consumer/consumer.go" },
    { "from": "/internal/dto/dto.go.tmpl", "to": "/internal/dto/dto.go" },
    { "from": "/internal/mapper/mapper.go.tmpl", "to": "/internal/mapper/mapper.go" },
    { "from": "/internal/validator/validator.go.tmpl", "to": "/internal/validator/validator.go" },
    { "from": "/pkg/constants/paths.go.tmpl", "to": "/pkg/constants/paths.go" },
    { "from": "/pkg/gen/proto/api.pb.go.stub.tmpl", "to": "/pkg/gen/proto/api.pb.go" },
    { "from": "/pkg/gen/proto/api_grpc.pb.go.stub.tmpl", "to": "/pkg/gen/proto/api_grpc.pb.go" },
    { "from": "/ci/github/workflows.yml.tmpl", "to": "/.github/workflows/ci.yml", "ci": "github" },
    { "from": "/ci/gitlab/.gitlab-ci.yml.tmpl", "to": "/.gitlab-ci.yml", "ci": "gitlab" },
    { "from": "/deployments/docker-compose.yml.tmpl", "to": "/deployments/docker-compose.yml" },
    { "from": "/deployments/Dockerfile.tmpl", "to": "/deployments/Dockerfile" },
    { "from": "/build/k8s/api.Dockerfile.tmpl", "to": "/build/k8s/api.Dockerfile" },
    { "from": "/build/k8s/migrate.Dockerfile.tmpl", "to": "/build/k8s/migrate.Dockerfile" },
    { "from": "/build/k8s/scheduler.Dockerfile.tmpl", "to": "/build/k8s/scheduler.Dockerfile" },
    { "from": "/build/k8s/seeder.Dockerfile.tmpl", "to": "/build/k8s/seeder.Dockerfile" },
    { "from": "/build/k8s/worker.Dockerfile.tmpl", "to": "/build/k8s/worker.Dockerfile" },
    { "from": "/build/local/api.Dockerfile.tmpl", "to": "/build/local/api.Dockerfile" },
    { "from": "/build/local/scheduler.Dockerfile.tmpl", "to": "/build/local/scheduler.Dockerfile" },
    { "from": "/build/local/worker.Dockerfile.tmpl", "to": "/build/local/worker.Dockerfile" },
    { "from": "/build/local/.air.toml.tmpl", "to": "/build/local/.air.toml" },
    { "from": "/build/README.md.tmpl", "to": "/build/README.md" },
    { "from": "/api/api.swagger.json.tmpl", "to": "/api/api.swagger.json" }
  ]
}
